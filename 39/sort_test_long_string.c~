#include <stdio.h>
#include <stdlib.h>
#include <string.h>
 
void solution(char *line)
{
	int i, a = 0, b = 0, c = 0;

	for (i = 0; i < strlen(line); i++) {
		if (line[i] == 'a')
			a++;
		else if (line[i] == 'b')
			b++;
		else if (line[i] == 'c')
			c++;
	}

	for (i = 0; i < a; i++)
		printf("a");
	for (i = 0; i < b; i++)
		printf("b");
	for (i = 0; i < c; i++)
		printf("c");
	printf("\n");
}
 
int main()
{
	char *line;

	line = calloc(102400, sizeof(char));

	strcpy(line, "");

	//scanf("%s", line);

	solution(line);
 
	return 0;
}
